{
  "name": "vuepress-theme-ououe-template",
  "version": "1.4.1",
  "description": "A template for vuepress-theme-ououe",
  "author": "2f0833e717 <skinoshita202001082135@gmail.com>",
  "repository": "https://github.com/tolking/vuepress-theme-ououe-template",
  "bugs": {
    "url": "https://github.com/tolking/vuepress-theme-ououe-template/issues"
  },
  "homepage": "https://github.com/tolking/vuepress-theme-ououe-template",
  "doc": "https://ououe.com/lib/vuepress-theme-ououe.html",
  "keywords": [
    "vue",
    "vuepress",
    "theme",
    "blog",
    "ououe"
  ],
  "scripts": {
    "postinstall": "npm run start",
    "start": "vuepress dev blog",
    "dev": "npm start",
    "build": "vuepress build blog",
    "deploy": "npx gh-pages -d dist -b master",
    "ga": "git add .",
    "gc": "git commit -m 'commit'",
    "gp": "git push origin master --force",
    "gacp": "npm run ga && npm run gc && npm run gp",
    "deploy-push": "npm run deploy & npm run gacp",
    "all": "npm run build && npm run deploy-push",
    "allb": "bash deploy.sh"
  },
  "dependencies": {
    "vuepress": "^1.2.0",
    "vuepress-plugin-container": "^2.1.1",
    "vuepress-theme-ououe": "^1.4.1"
  }
}

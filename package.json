{
  "name": "2f0833e717.github.io",
  "version": "1.4.1",
  "description": "vue-blog",
  "author": "2f0833e717 <skinoshita202001082135@gmail.com>",
  "repository": "https://github.com/2f0833e717/2f0833e717.github.io/tree/gh-pages",
  "bugs": {
    "url": "https://github.com/2f0833e717/2f0833e717.github.io/issues"
  },
  "homepage": "https://2f0833e717.github.io./",
  "doc": "https://ououe.com/lib/vuepress-theme-ououe.html",
  "keywords": [
    "vue",
    "vuepress",
    "theme",
    "blog",
    "ououe"
  ],
  "scripts": {
    "----------------00-Special-scripts": "",
    "postinstall": "git config --list && git branch master && git branch -a && npm start",
    "start": "git branch -a && vuepress dev blog",
    "----------------01-dev": "",
    "dev": "npm start",
    "----------------02-00-ci/cd = build-push-deploy": "",
    "----------------02-01-build": "",
    "build": "vuepress build blog",
    "----------------02-02-push": "",
    "push": "npm run g-push-gh-pages && npm run g-push-master",
    "g-push-gh-pages": "npm run g-add-cm && git push origin gh-pages --force",
    "g-add-cm": "git add . && git commit -m 'commit'",
    "g-push-master": "npm run g-merge && npm run g-add-cm && git push origin master --force",
    "g-merge": "git checkout master && git checkout --theirs . && git checkout gh-pages",
    "----------------02-03-deploy": "",
    "deploy": "npm run deploy-dist",
    "deploy-dist": "npx gh-pages -d dist -b master",
    "deploy-fix1-no-use": "git push -d origin gh-pages",
    "deploy-fix2-no-use": "git push origin gh-pages --force",
    "deploy-fix3-no-use": "git checkout -b gh-pages",
    "deploy-fix4-no-use": "git branch -D gh-pages",
    "----------------03-utility": "",
    "gl": "git log -7 --graph --oneline",
    "gref": "git reflog --all",
    "greset": "git reset --HARD @{1}",
    "all": "npm run build && npm run push && npm run deploy",
    "----------------04-alias": "",
    "a": "npm run all",
    "----------------05": ""
  },
  "dependencies": {
    "vuepress": "^1.2.0",
    "vuepress-plugin-container": "^2.1.1",
    "vuepress-theme-ououe": "^1.4.1"
  }
}
